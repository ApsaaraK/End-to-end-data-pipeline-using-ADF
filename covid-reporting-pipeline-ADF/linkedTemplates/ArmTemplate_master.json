{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-reporting-pipeline-ADF"
		},
		"ls_ablob_covid_reporting_pipeline_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covid_reporting_pipeline_sa'"
		},
		"ls_adls_covid_reporting_pipeline__dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covid_reporting_pipeline__dl'"
		},
		"ls_Http_opendata_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"ls_adls_covid_reporting_pipeline__dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingpipelinedl.dfs.core.windows.net/"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/30e75d65-af19-4f03-82d3-ab9c7992a3b2/resourceGroups/Covid-reporting-Rg/providers/Microsoft.Storage/storageAccounts/covidreportingpipelinesa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-reporting-pipeline-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covid_reporting_pipeline_sa_connectionString": {
						"value": "[parameters('ls_ablob_covid_reporting_pipeline_sa_connectionString')]"
					},
					"ls_adls_covid_reporting_pipeline__dl_accountKey": {
						"value": "[parameters('ls_adls_covid_reporting_pipeline__dl_accountKey')]"
					},
					"ls_Http_opendata_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_Http_opendata_europa_eu_properties_typeProperties_url')]"
					},
					"ls_adls_covid_reporting_pipeline__dl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covid_reporting_pipeline__dl_properties_typeProperties_url')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}